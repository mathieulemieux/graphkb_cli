export GKB_WORKDIR=$(cd $(dirname $0) && pwd)

if [[ $1 != '' ]]; then
    api=$1
else
    api=$(jq -c ".default_api" $GKB_WORKDIR/config)
fi

url=$(jq -c ".url.$api" $GKB_WORKDIR/config)

if [[ $url != null ]]; then
    echo $url
fi
